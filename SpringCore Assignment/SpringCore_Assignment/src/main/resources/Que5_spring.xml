<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
        
        <context:annotation-config />
        
        <bean id="Company" class="Que5.Company">
        <property name="name" value="Capgemini" />
        <property name="location" value="Mumbai" />
        </bean> 
        
        <bean id="Employee" class="Que5.Employee">
        <property name="id" value="416223" />
        <property name="name" value="Amruta" />
        </bean>
        
        <bean id="Address" class="Que5.Address">
		  <property name="street" value="Malvadi" />
		  <property name="city" value="Kolhapur" />
		  <property name="state" value="Maharashtra" />
		 </bean>
        
        
        
</beans>